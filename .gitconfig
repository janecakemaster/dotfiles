[push]
    default = simple
[core]
    excludesfile = ~/.gitignore_global
    editor = vim
[color]
    ui = auto
[color "branch"]
    current = yellow
    local = yellow
    remote = magenta
[color "diff"]
    meta = yellow bold
    frag = magenta bold
    old = red reverse
    new = green reverse
    whitespace = white reverse
[color "status"]
    added = green
    changed = yellow
    untracked = cyan reverse
    branch = magenta
[user]
    name = 'Jane Kim'
    email = jane@janekim.me
