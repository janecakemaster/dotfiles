[user]
    name = Jane Cakemaster
    email = janecakemaster@gmail.com

[credential]
    helper = osxkeychain

[core]
    excludefile = ~/.global_gitignore

[color]
    ui = auto

[color "branch"]
    current = yellow
    local = yellow
    remote = magenta

[color "diff"]
    meta = yellow bold
    frag = magenta bold
    old = red reverse
    new = green reverse
    whitespace = white reverse

[color "status"]
    added = green
    changed = yellow
    untracked = cyan reverse
    branch = magenta
[push]
	default = simple
